<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
<RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#7c7c7c" >

        <RadioButton
            android:id="@+id/wodexiaoxi_fanhui"
            android:layout_width="80dp"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/wode_title_btn_selector"
            android:button="@null"
            android:drawableLeft="@drawable/fanhui"
            android:gravity="left|center_vertical"
            android:paddingLeft="5dp"
            android:textColor="#fbfdfc"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal"
            android:text="资金详情"
            android:textColor="#fbfdfc"
            android:textSize="22sp" />
    </RelativeLayout>
  

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <TextView
            
            android:id="@+id/textView1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_height="match_parent"
            android:text="资金总额" />

        <TextView
            android:id="@+id/ziji_zonge"
            android:gravity="center"
           android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="match_parent"
            android:text="TextView" />

        <RadioButton
            android:id="@+id/zijin_tixian"
            android:layout_width="0dp"
            android:gravity="center"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:background="@drawable/sel"
             android:button="@null"
            android:layout_height="match_parent"
            android:text="提现" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        android:paddingLeft="10dp"
        android:layout_width="match_parent"
        android:gravity="center_vertical|bottom"
        android:layout_height="40dp"
        android:text="资金详情" />

    <ListView
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/zijin_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
    </ListView>

</LinearLayout>
