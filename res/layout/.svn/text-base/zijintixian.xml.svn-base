<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#cccccc"
    android:orientation="vertical" >
    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#7c7c7c" >

        <RadioButton
            android:id="@+id/hangpingguanli_fanhui"
            android:layout_width="80dp"
            android:layout_height="fill_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/wode_title_btn_selector"
            android:button="@null"
            android:drawableLeft="@drawable/fanhui"
            android:gravity="left|center_vertical"
            android:paddingLeft="5dp"
            android:textColor="#fbfdfc"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal"
            android:text="提现明细"
            android:textColor="#fbfdfc"
            android:textSize="22sp" />
        
        
        
      <RadioButton
            android:id="@+id/tixian_mingxi"
            android:layout_width="80dp"
            android:layout_height="fill_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/wode_title_btn_selector"
            android:button="@null"
            android:text="提现明细"
            android:gravity="left|center_vertical"
            android:paddingLeft="5dp"
            android:textColor="#fbfdfc"
            android:textSize="15sp" />  
        
        
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_height="match_parent"
            android:text="账户余额:" />

        <TextView
            android:id="@+id/zijiyue_text"
             android:layout_weight="1"
            android:layout_width="0dp"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_height="match_parent"
            android:text="" />
        
    </LinearLayout>
                <TextView android:layout_width="match_parent"
                    android:layout_height="1dp"
                   android:layout_marginLeft="10dp"/>

                
          <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
       android:focusable="true"
       android:focusableInTouchMode="true"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_height="match_parent"
            android:text="提现额度:" />

        <EditText
            android:id="@+id/zijin_tixian_ed1"
             android:layout_weight="1"
            android:layout_width="0dp"
            android:textSize="12sp"
           android:phoneNumber="true"
            android:hint="请输入需要提现的额度"
            android:gravity="center_vertical"
            android:paddingLeft="10dp"
            android:layout_height="match_parent"
            />
        
    </LinearLayout>
                <TextView android:layout_width="match_parent"
                    android:layout_height="1dp"
                   android:layout_marginLeft="10dp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp" >

                    <RadioButton
                        android:id="@+id/querentixian_butt1"
                        android:layout_width="250dp"
                        android:layout_height="match_parent"
                       
                        android:background="@drawable/sel"
                        android:button="@null"
                        android:gravity="center"
                        android:text="确认提现" />
                </LinearLayout>

                <ListView
                    android:background="#ffffff"
                    android:layout_marginTop="20dp"
                    android:id="@+id/zijin_tixian_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >
                </ListView>
   
</LinearLayout>
